{
	"name": "@charlesshook/strapi-blocks-svelte-renderer",
	"version": "0.1.0",
	"description": "Strapi block renderer for Svelte",
		"keywords": [
		"svelte",
		"strapi",
		"blocks",
		"renderer"
	],
	"author": "Charles Shook (https://charlesshook.com/)",
	"homepage": "https://github.com/charlesshook/strapi-blocks-svelte-renderer#readme",
  "bugs": {
    "url": "https://github.com/charlesshook/strapi-blocks-svelte-renderer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:charlesshook/strapi-blocks-svelte-renderer.git"
  },
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.6"
	}
}
